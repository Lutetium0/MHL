	<topmodule name="cfg"/>
    
    <option name="editor.topmodule" value="editor.cfg"/>

	<patch name="cn"/>
	<patch name="tw"/>
	<patch name="en"/>
	<patch name="jp"/>

	<group name="c" default="1"/> client
	<group name="s" default="1"/> server
	<group name="e" default="1"/> editor
	
	<import name="."/>
	
	<importexcel name="__tables__.xlsx" type="table"/> 相对data目录
